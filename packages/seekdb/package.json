{
  "name": "seekdb",
  "version": "1.1.0",
  "description": "A unified JavaScript/TypeScript SDK for seekdb that supports Server mode and OceanBase mode",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "build:bindings": "cd ../bindings && node-gyp configure && node-gyp build",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write .",
    "docs": "typedoc --entryPoints src/index.ts --out docs/v2"
  },
  "keywords": [
    "seekdb",
    "vector-database",
    "embeddings",
    "similarity-search",
    "hybrid-search"
  ],
  "files": [
    "dist"
  ],
  "author": "OceanBase",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "mysql2": "^3.11.5",
    "@seekdb/js-bindings": "workspace:*"
  },
  "devDependencies": {
    "@seekdb/default-embed": "latest"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oceanbase/seekdb-js.git",
    "directory": "packages/seekdb"
  },
  "homepage": "https://github.com/oceanbase/seekdb-js#readme",
  "bugs": {
    "url": "https://github.com/oceanbase/seekdb-js/issues"
  }
}
